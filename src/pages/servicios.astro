---
import BaseLayout from '../layouts/BaseLayout.astro';
import ProductsServicesPage from '../components/templates/ProductsServicesPage.astro';

const seoData = {
  title: "Productos y Servicios - PIXII International Power Supplies",
  description: "Descubre nuestro catálogo de productos y servicios especializados en equipos eléctricos de potencia. Transformadores, celdas de media tensión, sistemas de protección, instalación, mantenimiento y consultoría técnica.",
